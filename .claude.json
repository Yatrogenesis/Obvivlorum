      "test_standard": "python -m pytest tests/ -v",
      "test_research": "python -m pytest research_tests/ -m 'not slow'",
      "test_benchmark": "python -m pytest benchmarks/ --benchmark-only",
      "test_all": "python -m pytest tests/ research_tests/ benchmarks/",
      
      "lint": "ruff check . --fix",
      "typecheck": "mypy .",
      "format": "black .",
      
      "build_exe": "python -m PyInstaller --onefile ai_symbiote.py",
      "build_portable": "./CREATE_PORTABLE_VERSION.bat",
      "build_docker": "docker build -t obvivlorum .",
      
